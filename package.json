{
  "name": "react-admin-vite",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm install",
    "serve": "pnpm run dev",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production tsc && vite build && tsx ./build/script/postBuild.ts",
    "build:test": "cross-env vite build --mode test && tsx ./build/script/postBuild.ts",
    "build:no-cache": "yarn clean:cache && npm run build",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "test:br": "npx http-server dist --cors --brotli -c-1",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "prepare": "husky install",
    "gen:icon": "tsx ./build/generate/icon/index.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@antv/x6": "^2.11.1",
    "@antv/x6-react-shape": "^2.2.2",
    "@dagrejs/dagre": "^1.0.2",
    "@loadable/component": "^5.15.3",
    "@react-spring/core": "^9.7.2",
    "@react-spring/three": "^9.7.2",
    "@react-three/drei": "^9.77.0",
    "@react-three/fiber": "^8.13.3",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.6.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "i18next": "^22.5.0",
    "jsbarcode": "^3.11.5",
    "less": "^4.1.3",
    "pinyin-match": "^1.2.4",
    "qrcode": "^1.5.3",
    "qrcode.react": "^3.1.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-cropper": "^2.3.3",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-i18next": "^12.0.0",
    "react-pdf": "^7.1.2",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "react-select": "^5.7.3",
    "react-use": "^17.4.0",
    "rxjs": "^7.5.7",
    "styled-components": "^5.3.11",
    "throttle-debounce": "^5.0.0",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/styled-components": "^5.1.26",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-istanbul": "^0.31.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "inquirer": "^9.2.3",
    "jsdom": "^22.0.0",
    "mockjs": "^1.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2",
    "vite": "4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.31.0"
  }
}
